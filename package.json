{
  "name": "mern-boilerplate",
  "version": "1.0.2",
  "main": "index.js",
  "author": "VitalyK",
  "license": "MIT",
  "scripts": {
    "dev": "clear & yarn run watch:server & yarn run watch:client",
    "watch:server": "nodemon start.js",
    "watch:client": "webpack serve --config webpack.dev.js",
    "lint": "eslint './server/**/*.js' './client/**/*.js' './client/**/*.jsx'",
    "prettier": "prettier './server/**/*.js' './client/**/*.js' './client/**/*.jsx' --write --no-error-on-unmatched-pattern",
    "build": "cross-env NODE_ENV=production webpack --config webpack.prod.js --progress --profile",
    "start": "cross-env NODE_ENV=production && node start.js",
    "heroku-postbuild": "yarn run build:prod",
    "prepare": "husky install"
  },
  "dependencies": {
    "@babel/core": "^7.16.5",
    "@babel/eslint-parser": "^7.16.5",
    "@babel/plugin-transform-runtime": "^7.16.5",
    "@babel/preset-env": "^7.16.5",
    "@babel/preset-react": "^7.16.5",
    "@babel/register": "^7.16.5",
    "@babel/runtime": "^7.16.5",
    "babel-loader": "^8.2.2",
    "connected-react-router": "^6.9.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "eslint-config-prettier": "^8.3.0",
    "express": "^4.17.2",
    "history": "4.10.1",
    "mongoose": "^5.12.3",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.4.1",
    "serve-favicon": "^2.5.0",
    "socket.io": "^4.4.0",
    "socket.io-client": "^4.4.0"
  },
  "devDependencies": {
    "autoprefixer": "^10.2.0",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^10.2.0",
    "css-loader": "^6.5.1",
    "css-minimizer-webpack-plugin": "^3.3.0",
    "eslint": "^8.5.0",
    "eslint-config-airbnb": "^19.0.2",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-webpack-plugin": "^3.1.1",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^7.0.4",
    "mini-css-extract-plugin": "^2.4.5",
    "node-sass": "^6.0.1",
    "nodemon": "^2.0.15",
    "postcss": "^8.4.5",
    "postcss-import": "^14.0.0",
    "postcss-loader": "^6.2.1",
    "postcss-preset-env": "^7.0.2",
    "prettier": "^2.5.1",
    "sass-loader": "^12.4.0",
    "tailwindcss": "^3.0.7",
    "terser-webpack-plugin": "^5.3.0",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.7.0",
    "webpack-dev-server": "^4.6.0",
    "webpack-merge": "^5.7.3"
  }
}
